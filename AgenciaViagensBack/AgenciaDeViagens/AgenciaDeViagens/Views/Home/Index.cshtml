@{
    ViewData["Title"] = "Home";
}
<link rel="stylesheet" href="~/css/StyleSheet.css">

<h1 align="center" class="text-primary">
    Principais destinos internacionais dessa temporada
</h1>

<div id="carouselExampleControls" class="carousel" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/css/Imagens/1.png" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="~/css/Imagens/3.png" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="~/css/Imagens/4.png" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="~/css/Imagens/5.png" class="d-block w-100" alt="...">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
